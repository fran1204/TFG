<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="user.xml"/>
    <include file="lot.xml"/>
    <include file="advertising.xml"/>
    <include file="navegacion.xml"/>
    <include file="order.xml"/>
    <include file="lotdetail.xml"/>
    <include file="email.xml"/>
    <constant name="struts.multipart.maxSize" value="100000000000" /> 
    <constant name="struts.mapper.action.prefix.enabled" value="true" />
